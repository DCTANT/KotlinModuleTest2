<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/wholeItem"
				xmlns:android="http://schemas.android.com/apk/res/android"
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:background="@color/ucrop_white"
				android:layout_marginTop="5dp"
				android:layout_marginRight="5dp"
				android:orientation="vertical">


	<ImageView
		android:id="@+id/showPic"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_centerInParent="true"
		android:scaleType="centerCrop"/>
	<LinearLayout
		android:id="@+id/selectLl"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:layout_alignParentRight="true"
		android:layout_alignParentTop="true"
		android:paddingTop="6.67dp"
		android:paddingRight="6.67dp"
		android:paddingLeft="15dp"
		android:paddingBottom="15dp">
		<TextView
			android:id="@+id/checkSelect"
			android:layout_width="17dp"
			android:layout_height="17dp"
			android:gravity="center"
			android:textSize="@dimen/font_10"
			android:textColor="@color/ucrop_white"
			android:background="@drawable/shape_no_selected_num"/>
	</LinearLayout>






</RelativeLayout>